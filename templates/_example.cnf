[ req ]
default_bits = 2048
default_keyfile = __example.local__.key
encrypt_key = no
default_md = sha256
prompt = no
utf8 = yes
distinguished_name = distinguished_name
req_extensions = extensions

[ distinguished_name ]
C=SE
ST=Region
L=City
O=Dev
CN=*.__example.local__

[ extensions ]
basicConstraints=CA:FALSE
subjectKeyIdentifier=hash
subjectAltName=@alt_names

[ alt_names ]
DNS.1 = *.__example.local__
DNS.2 = __example.local__
